@model Calcpad.web.ViewModels.ParseViewModel;
@{
    ViewData["Title"] = "Print Preview";
}

@section Meta{
    <meta name="description" content="Calcpad online - print preview">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
}
@section Styles{
    <style>
        body {
            background-color: #ddd;
        }
    </style>
}
<div class="Calcpad container" id="Preview">
    <div class="no-print lead float-right position-relative" style="top: -5mm; right: -5mm;">
        <a href="javascript:window.print()"><i class="icofont-print text-primary mr-2"></i></a>
        <a href="javascript:window.close()"><i class="icofont-close text-primary"></i></a>
    </div>
    @Model.Output
</div>
@section Scripts{
    <environment include="Development">
        <script src="~/js/calcpad/fold.js"></script>
        <script src="~/js/calcpad/select.js"></script>
        <script src="~/js/calcpad/units.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/calcpad/fold.min.js"></script>
        <script src="~/js/calcpad/select.min.js"></script>
        <script src="~/js/calcpad/units.min.js"></script>
    </environment>
}